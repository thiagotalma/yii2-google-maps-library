{
    "name": "thiagotalma/yii2-google-maps-library",
    "description": "Google Maps API library for Yii2.",
    "keywords": [
        "yii",
        "extension",
        "library",
        "google",
        "maps"
    ],
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "thiagotalma",
            "email": "hola@thiagotalma.us",
            "homepage": "http://thiagotalma.us"
        },
        {
            "name": "Antonio Ramirez",
            "email": "hola@thiagotalma.us",
            "homepage": "https://thiagotalma.us"
        }
    ],
    "require": {
        "ext-json": "*",
        "yiisoft/yii2": ">=2.0.13",
        "guzzlehttp/guzzle": "6.*",
        "ext-SimpleXML": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10@dev"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\google\\maps\\": ""
        }
    },
    "config": {
        "sort-packages": true
    }
}
